; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Basic RPG"
config/version="1.0.0"
run/main_scene="res://root.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60

[autoload]

LevelManager="*res://00_Globals/GlobalLevelManager.gd"
PlayerHud="*res://GUI/player_hud/player_hud.tscn"
PlayerManager="*res://00_Globals/GlobalPlayerManager.gd"
SceneTransition="*res://GUI/scene_transition/scene_transition.tscn"
SaveManager="*res://00_Globals/GlobalSaveManager.gd"
OpeningMenu="*res://00_Main_Menu/MainMenu/main_menu.tscn"
PauseMenu="*res://GUI/pause_menu/pause_menu.tscn"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/mode=2
window/size/extend_to_title=true
window/size/window_width_override=1960
window/size/window_height_override=1080
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Basic RPG"

[file_customization]

folder_colors={
"res://00_Globals/": "blue",
"res://Assets/": "purple",
"res://Enemies/": "red",
"res://GUI/": "blue",
"res://GeneralNodes/": "gray",
"res://Items/": "yellow",
"res://Levels/": "green",
"res://Player/": "pink",
"res://Props/": "yellow",
"res://Tile Maps/": "green"
}

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

